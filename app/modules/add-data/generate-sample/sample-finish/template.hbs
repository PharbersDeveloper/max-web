{{outlet}}
{{#layout-push direction='row' classNames="layout-flex-outer-auto-height"}}
	{{#layout-push direction='row' classNames="layout-flex-inner"}}
		{{#layout-push direction='col' classNames="sample-finish-outer"}}
			{{#layout-push direction='row' classNames="sample-finish-choosebar"}}
				{{#layout-push direction='row' classNames="sample-finish-choosebar-left"}}
					<p local-class="select-title">市场</p>
					{{!-- <select local-class="choose-select choose-market" name="markets" {{action 'queryAll' on='change'}}>
                            {{#each markets as |item|}}
                                <option value={{item}}>{{item}}</option>
                            {{/each}}
                        </select> --}}
					{{base-select title="" selected=market onChange=(action (mut market)) options=markets}}
					<p local-class="select-title">月份</p>
					{{!-- <select local-class="choose-select choose-month" name="years" {{action 'queryAll' on='change'}}>
                            {{#each years as |item|}}
                                <option value={{item}}>{{item}}</option>
                            {{/each}}
                        </select> --}}
					{{base-select title="" selected=year onChange=(action (mut year)) options=years}}
					{{base-button content="导出" type="" onClick=""}}
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-choosebar-right"}}
					{{base-button content="重新上传" type="" onClick=(action (mut uploadAgain) true)}}
					{{base-button content="下一步" type="" onClick=(action 'next')}}
				{{/layout-push}}
			{{/layout-push}}
			{{#layout-push direction='col' classNames="sample-finish-graph"}}
				{{#layout-push direction='row' classNames="sample-finish-graph-title"}}
					<p local-class="item-title">{{selectedMarket}} 市场 医院样本数量</p>
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-info"}}
					{{#layout-push direction='row' classNames="sample-finish-graph-left"}}
						<div local-class="legend-left">
							<span></span><span local-class="text">本月样本数</span><span>---</span><span local-class="text">去年各月样本数趋势</span>
						</div>
					{{/layout-push}}
					{{#layout-push direction='row' classNames="sample-finish-graph-right"}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-left"}}
							<p local-class={{if (or (gt computeHospitalNumber 5) (lt computeHospitalNumber -5)) "error" "success"}}>{{computeHospitalNumber}}%</p>
							<p>样本误差率</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-mid"}}
							<p>{{hospitalNumber}}<span>家</span></p>
							<p>本月样本数</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-right"}}
							<p>{{lastYearHospitalNumber}}<span>家</span></p>
							<p>去年同月样本数</p>
						{{/layout-push}}
					{{/layout-push}}
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-content"}}
					{{hosp-amount dataset=hosp_datas}}
				{{/layout-push}}
			{{/layout-push}}
			{{#layout-push direction='col' classNames="sample-finish-graph"}}
				{{#layout-push direction='row' classNames="sample-finish-graph-title"}}
					<p local-class="item-title">{{selectedMarket}} 市场 产品样本数量</p>
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-info"}}
					{{#layout-push direction='row' classNames="sample-finish-graph-left"}}
						<div local-class="legend-left">
							<span></span><span local-class="text">本月样本数</span><span>---</span><span local-class="text">去年各月样本数趋势</span>
						</div>
					{{/layout-push}}
					{{#layout-push direction='row' classNames="sample-finish-graph-right"}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-left"}}
							<p local-class={{if (or (gt computeProductNumber 5) (lt computeProductNumber -5)) "error" "success"}}>{{computeProductNumber}}%</p>
							<p>样本误差率</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-mid"}}
							<p>{{productNumber}}<span>个</span></p>
							<p>本月样本数</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-right"}}
							<p>{{lastYearProductNumber}}<span>个</span></p>
							<p>去年同月样本数</p>
						{{/layout-push}}
					{{/layout-push}}
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-content"}}
					{{product-amount dataset=pro_datas}}
				{{/layout-push}}
			{{/layout-push}}
			{{#layout-push direction='col' classNames="sample-finish-graph"}}
				{{#layout-push direction='row' classNames="sample-finish-graph-title"}}
					<p local-class="item-title">{{selectedMarket}} 市场 样本销售额</p>
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-info"}}
					{{#layout-push direction='row' classNames="sample-finish-graph-left"}}
						<div local-class="legend-left">
							<span></span><span local-class="text">本月样本数</span><span>---</span><span local-class="text">去年各月样本数趋势</span>
						</div>
					{{/layout-push}}
					{{#layout-push direction='row' classNames="sample-finish-graph-right"}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-left"}}
							<p local-class={{if (or (gt computeSalesNumber 5) (lt computeSalesNumber -5)) "error" "success"}}>{{computeSalesNumber}}%</p>
							<p>样本误差率</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-mid"}}
							<p>{{salesNumber}}<span>(Mil)</span></p>
							<p>本月样本数</p>
						{{/layout-push}}
						{{#layout-push direction='col' classNames="sample-finish-graph-right-right"}}
							<p>{{lastYearSalesNumber}}<span>(Mil)</span></p>
							<p>去年同月样本数</p>
						{{/layout-push}}
					{{/layout-push}}
				{{/layout-push}}
				{{#layout-push direction='row' classNames="sample-finish-graph-content"}}
					{{sample-sales dataset=sale_datas}}
				{{/layout-push}}
			{{/layout-push}}
			{{#layout-push direction='col' classNames="sample-finish-table"}}
				{{#layout-push direction='row' classNames="sample-finish-table-title"}}
					<p local-class="item-title">样本医院列表</p>
				{{/layout-push}}
				{{sample-table model=prodSalesValue columns=prodSales selfheight='300px'}}
			{{/layout-push}}
		{{/layout-push}}
	{{/layout-push}}
{{/layout-push}}
